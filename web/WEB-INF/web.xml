<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>contactServlet</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.contactServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signupServlet</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.signupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signinServlet</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.signinServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editProfileServlet</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.editProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>askQuestionServlet</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.askQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>postAnsServlet</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.postAnsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editQAServlet</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.editQAServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteQAServlet</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.deleteQAServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editPostServlet</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.editPostServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletePostServelt</servlet-name>
        <servlet-class>com.stackuncontrol.servlet.deletePostServelt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>contactServlet</servlet-name>
        <url-pattern>/contactServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signupServlet</servlet-name>
        <url-pattern>/signupServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signinServlet</servlet-name>
        <url-pattern>/signinServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editProfileServlet</servlet-name>
        <url-pattern>/editProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>askQuestionServlet</servlet-name>
        <url-pattern>/askQuestionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>postAnsServlet</servlet-name>
        <url-pattern>/postAnsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editQAServlet</servlet-name>
        <url-pattern>/editQAServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteQAServlet</servlet-name>
        <url-pattern>/deleteQAServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editPostServlet</servlet-name>
        <url-pattern>/editPostServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletePostServelt</servlet-name>
        <url-pattern>/deletePostServelt</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <!--Remove file extension web script start-->
    <servlet>
        <servlet-name>/</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>/</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <!--Remove file extension web script end-->
</web-app>
